pipeline{
	agent any
		stages{
			stage('build'){
				steps{
					sh 'mvn clean'
				}
			}
			stage('compile'){
				steps{
					sh 'mvn compile'
				}
			}
			stage('package'){
				steps{
					sh 'mvn package'
				}
			}
			stage('deploying'){
				steps{
					sh 'mvn cargo:undeploy'
					sh 'mvn clean'
					sh 'mvn install'
					sh 'mvn cargo:deploy'
				}
			}
		}
	}
