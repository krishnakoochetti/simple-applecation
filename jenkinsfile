pipeline {
  agent any
  stages {
    stage ('Build') {
      steps {
        sh 'mvn clean compile package'
      }
    }
    stage ('login') {
      steps {
        sh 'sudo su'
        sh 'read 3010'
        sh 'docker exec -it 5f95031cf797 bash'
        sh 'cd webapps'
        sh 'ls'
  
      }
    }
  }
}
