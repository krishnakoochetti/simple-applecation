pipeline {
  agent any
  stages {
    stage ('Build') {
      steps {
        sh 'mvn clean compile package'
      }
    }
    
    stage ('pulling') {
      steps {
        sh 'docker pull 9959087127/spring-docker:latest'
      }
    }
    
    stage ('login') {
      steps {
        withCredentials([usernamePassword(credentialsId: 'docker', usernameVariable: 'USER', passwordVariable: 'PASSWORD')])
			{
				sh 'docker login --SSH username $USER --private key $PASSWORD'
        sh 'echo login successful'
			}
  
      }
    }
  }
}
